set nocompatible  "Turn off all the compat stuff

colorscheme elflord

"Set indeent style from filetype
filetype indent plugin on

syntax on

set hidden

set wildmenu
set showcmd
set hlsearch

set ignorecase
set smartcase

set autoindent
set ruler
set confirm

set mouse=a

set cmdheight=2

set shiftwidth=4
set softtabstop=4
set expandtab
