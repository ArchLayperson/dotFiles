"pactl set-sink-volume @DEFAULT_SINK@ +2%"
    XF86AudioRaiseVolume
"pactl set-sink-volume @DEFAULT_SINK@ -2%"
    XF86AudioLowerVolume
"pactl set-sink-mute @DEFAULT_SINK@ toggle"
    XF86AudioMute
"xbacklight +5"
    XF86MonBrightnessUp
"xbacklight -5"
    XF86MonBrightnessDown
"numlockx; speedcrunch"
    XF86Calculator
"playerctl play-pause"
    XF86AudioPlay
"playerctl previous"
    XF86AudioPrev
"playerctl next"
    XF86AudioNext
